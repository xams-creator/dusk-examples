{
    "name": "dusk-example-vite-react",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "vite --force",
        "build": "tsc && vite build --outDir=build",
        "preview": "vite preview --outDir=build"
    },
    "dependencies": {
        "@xams-framework/dusk": "1.2.1",
        "@xams-framework/dusk-plugin-loading": "^1.0.1",
        "@xams-framework/dusk-plugin-hmr": "^1.1.3",
        "@xams-framework/dusk-plugin-context-vite": "^0.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/node": "^18.11.19",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/redux-logger": "^3.0.9",
        "@vitejs/plugin-react": "^3.1.0",
        "@xams-framework/vite-plugin-dusk": "^0.0.4",
        "@types/lodash": "^4.14.191",
        "postcss-preset-env": "^8.0.1",
        "prettier": "^2.8.3",
        "sass": "^1.58.0",
        "typescript": "^4.9.5",
        "vite": "^4.1.1",
        "vite-plugin-eslint": "^1.8.1",
        "eslint": "^8.35.0",
        "eslint-config-react-app": "^7.0.1"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "rules": {
            "jsx-a11y/anchor-is-valid": "off",
            "no-undef": "off",
            "no-restricted-globals": "off",
            "no-unused-vars": "off",
            "no-new-func": "off",
            "import/no-anonymous-default-export": "off",
            "@typescript-eslint/no-unused-vars": "off",
            "import/no-amd": "off"
        }
    }
}
