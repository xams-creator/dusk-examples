{
    "name": "dusk-example-structure",
    "version": "0.0.0",
    "private": true,
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "@xams-framework/dusk": "1.2.1",
        "@xams-framework/common": "^0.1.3",
        "@xams-framework/dusk-plugin-hmr": "^1.1.3",
        "@xams-framework/dusk-plugin-context-webpack": "^0.0.1",
        "@xams-framework/dusk-plugin-loading": "^1.0.2",
        "antd": "^5.5.2",
        "lodash": "^4.17.21",
        "react-flip-move": "^3.0.3",
        "ahooks": "^3.7.6"
    },
    "devDependencies": {
        "@xams-framework/craco-plugin-dusk-hmr": "1.0.1",
        "@types/redux-logger": "^3.0.9",
        "@types/lodash": "^4.14.188",
        "@craco/craco": "^7.1.0",
        "@craco/types": "^7.1.0",
        "react-scripts": "^5.0.1",
        "@types/node": "^16.11.66",
        "@types/react": "^18.0.21",
        "sass": "^1.62.1",
        "@types/react-dom": "^18.0.6",
        "typescript": "^4.9.4",
        "prettier": "^2.8.7",
        "yorkie": "^2.0.0",
        "lint-staged": "^13.1.0"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "craco test"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "jsx-a11y/anchor-is-valid": "off",
            "no-undef": "off",
            "no-restricted-globals": "off",
            "no-unused-vars": "off",
            "no-new-func": "off",
            "import/no-anonymous-default-export": "off",
            "@typescript-eslint/no-unused-vars": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
